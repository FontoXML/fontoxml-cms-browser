{
	"show-open-document-browser-plus-create-document-form-tabs-modal": {
		"initialData": {
			"openDocumentDataProviderName": "dataProviderUsingConfiguredConnectorsForDocuments",
			"selectDocumentTemplateDataProviderName": "dataProviderUsingConfiguredConnectorsForDocumentTemplates",
			"selectFolderDataProviderName": "dataProviderUsingConfiguredConnectorsForDocumentFolders"
		},
		"steps": {
			"type": "modal/FxOpenOrCreateDocumentModalStack",
			"options": {
				"controller": "FxOpenOrCreateDocumentModalStackController",
				"templateUrl": "fontoxml-file-or-folder-modals/modals/documents/fx-open-or-create-document-modal-stack-template.html",
				"windowClass": "fds-modal-fullscreen"
			},
			"data": {
				"browseContextDocumentId": "{{browseContextDocumentId}}",
				"openDocumentDataProviderName": "{{openDocumentDataProviderName}}",
				"selectDocumentTemplateDataProviderName": "{{selectDocumentTemplateDataProviderName}}",
				"selectFolderDataProviderName": "{{selectFolderDataProviderName}}"
			}
		}
	},
	"show-create-document-form-modal": {
		"initialData": {
			"selectDocumentTemplateDataProviderName": "dataProviderUsingConfiguredConnectorsForDocumentTemplates",
			"selectFolderDataProviderName": "dataProviderUsingConfiguredConnectorsForDocumentFolders"
		},
		"steps": {
			"type": "modal/FxCreateDocumentFormModalStack",
			"options": {
				"controller": "FxCreateDocumentFormModalStackController",
				"templateUrl": "fontoxml-file-or-folder-modals/modals/documents/fx-create-document-form-modal-stack-template.html",
				"windowClass": "fds-modal-fullscreen"
			},
			"data": {
				"browseContextDocumentId": "{{browseContextDocumentId}}",
				"selectDocumentTemplateDataProviderName": "{{selectDocumentTemplateDataProviderName}}",
				"selectFolderDataProviderName": "{{selectFolderDataProviderName}}"
			}
		}
	},
	"show-open-document-browser-modal": {
		"initialData": {
			"dataProviderName": "dataProviderUsingConfiguredConnectorsForDocuments"
		},
		"steps": {
			"type": "modal/FxOpenDocumentBrowserModal",
			"options": {
				"controller": "FxOpenDocumentBrowserModalController",
				"templateUrl": "fontoxml-file-or-folder-modals/modals/documents/fx-open-document-browser-modal-template.html",
				"windowClass": "fds-modal-fullscreen"
			},
			"data": {
				"browseContextDocumentId": "{{browseContextDocumentId}}",
				"dataProviderName": "{{dataProviderName}}"
			}
		}
	},
	"show-document-link-browser-modal": {
		"initialData": {
			"dataProviderName": "dataProviderUsingConfiguredConnectorsForDocuments",
			"linkableElementsQuery": "//*[@id]"
		},
		"steps": {
			"type": "modal/FxDocumentLinkBrowserModal",
			"options": {
				"controller": "FxDocumentLinkBrowserModalController",
				"templateUrl": "fontoxml-file-or-folder-modals/modals/document-links/fx-document-link-browser-modal-template.html",
				"windowClass": "fds-modal-fullscreen"
			},
			"data": {
				"browseContextDocumentId": "{{browseContextDocumentId}}",
				"dataProviderName": "{{dataProviderName}}",
				"linkableElementsQuery": "{{linkableElementsQuery}}",
				"targetSpec": "{{targetSpec}}"
			}
		}
	},
	"show-image-browser-modal": {
		"initialData": {
			"dataProviderName": "dataProviderUsingConfiguredConnectorsForImages"
		},
		"steps": {
			"type": "modal/FxImageBrowserModal",
			"options": {
				"controller": "FxImageBrowserModalController",
				"templateUrl": "fontoxml-file-or-folder-modals/modals/images/fx-image-browser-modal-template.html",
				"windowClass": "fds-modal-fullscreen"
			},
			"data": {
				"browseContextDocumentId": "{{browseContextDocumentId}}",
				"dataProviderName": "{{dataProviderName}}",
				"targetSpec": "{{targetSpec}}"
			}
		}
	}
}
